{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "wazframe/cluster",
  "version": "0.2.0",
  "title": "Cluster",
  "category": "layout-primitives",
  "description": "Groups items in a cluster that wrap, such as navigation menu and tags",
  "textdomain": "wf-layout",
  "icon": "tagcloud",
  "attributes": {
    "tagName": {
      "type": "string",
      "default": "div"
    },
    "flex": {
      "type": "object",
      "justifyContent": {
        "type": "string",
        "default": "flex-start"
      },
      "alignItems": {
        "type": "string",
        "default": "flex-start"
      }
    },
    "blockGap": {
      "type": "string"
    },
    "templateLock": {
      "type": ["string", "boolean"],
      "enum": ["all", "insert", false]
    }
  },
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}
