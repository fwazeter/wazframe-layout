{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "wazframe/imposter",
  "version": "0.2.0",
  "title": "Imposter",
  "category": "layout-primitives",
  "description": "Imposter takes an element out of the layout flow, such as to obscure content or hover dialog.",
  "textdomain": "wf-layout",
  "icon": "admin-page",
  "attributes": {
    "position": {
      "type": "object"
    },
    "positionType": {
      "type": "string"
    },
    "contain": {
      "type": "boolean",
      "default": false
    },
    "margin": {
      "type": "string"
    },
    "templateLock": {
      "type": ["string", "boolean"],
      "enum": ["all", "insert", false]
    },
    "style": {
      "type": "object"
    }
  },
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}
